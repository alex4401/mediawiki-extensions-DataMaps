		"datamap-category-maps-failing-validation",
		"datamap-category-pages-including-broken-maps"
	],

	"namespaces": [
		{
			"id": 2900,
			"constant": "NS_MAP",
			"name": "Map",
			"defaultcontentmodel": "wikitext",
			"subpages": true,
			"conditional": true
